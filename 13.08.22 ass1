#include<stdio.h>
int main()
{
   char ch='A';
    char CH='a';
    int i,j,n;
    printf("Enter n value: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("%c%c\t",ch,CH);
            ch++;
            CH++;
        }
        printf("\n");
    }
   return 0;
}
