#include<stdio.h>
int main()
{
    int i,j,n;
    int array[100];
    printf("ENTER THE SIZE OF ARRAY:\n");
    scanf("%d",&n);
    printf("ENTER THE ELEMENTS OF AN ARRAY\n");
    for(i=n;i>0;i--)
    {
      scanf("%d",&array[i]);
    }
      printf("THE PRIME NUMBERS OF AN ARRAY IS\n");
       for(i=1;i<=n;i++)
     {
        int flag=0;
          if(array[i]!=1)
        {
           for(j=2;j<array[i];j++)
          {
             if(array[i]%j==0)
            {
                flag=1;
                break;
            }
          }
      
           if(flag==0)
           printf("%d\t",array[i]);
         }   
     }

    return 0;
    }
